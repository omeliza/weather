{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2022",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  }
}
